<?xml version="1.0" encoding="utf-8"?>
<configuration>
    <startup> 
        <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.5"/>
    </startup>
    <connectionStrings>
        <add name="ServerConnectionString" connectionString="Data Source=localhost\SQLEXPRESS;Initial Catalog=HostServiceDb;Integrated Security=True;" providerName="System.Data.SqlClient"/>
    </connectionStrings>
    <appSettings>
        <add key="LogDirectory" value="Logs"/>
        <add key="MaxLogFileSize" value="10"/>
        <add key="MaxLogFileCount" value="10"/>
    </appSettings>
</configuration>
