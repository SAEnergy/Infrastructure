        <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.5.2" />
    </startup>
    <connectionStrings>
        <add name="AppConnectionString" connectionString="Data Source=.\SQLExpress;Initial Catalog=HostServiceDatabase;Integrated Security=True;" providerName="System.Data.SqlClient"/>
